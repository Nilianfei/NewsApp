<?xml version="1.0" encoding="utf-8"?>
<com.example.al.newshub.PageEnabledSlidingPaneLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.al.newshub.MainActivity"
    android:id="@+id/slidingPanelayout">

    <fragment
        android:id="@+id/fragment_left_menu"
        android:name="com.example.al.newshub.Fragment.LeftMenuFragment"
        android:layout_width="200dp"
        android:layout_height="match_parent" />

    <fragment
        android:id="@+id/fragment_right"
        android:name="com.example.al.newshub.Fragment.RightFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</com.example.al.newshub.PageEnabledSlidingPaneLayout>
